@model List<ApplicationUser>
@if (Model.Count > 0)
{
	<div class="list-group">
	@foreach (ApplicationUser user in Model)
	{
			 <div class="list-group-item row" id="@("ID" + user.Id)">
				<div class="col-md-2">
					@user.DisplayName
				</div>
				<div class="col-md-offset-8 col-md-2">
					<button onclick="remove_relation('@user.Id')" class="btn btn-danger">
						Retirer
					</button>
				</div>
			 </div>
	}
	</div>
} else
{
	<div>
		Pas de contact enregistré
	</div>
}