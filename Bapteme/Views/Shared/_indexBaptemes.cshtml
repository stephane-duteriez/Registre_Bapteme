@model List<Bapteme.Models.ApplicationUser>
<div>
	@if (Model.Count() > 0)
	{
	<table class="table table-striped">
		<tbody>
				@foreach (ApplicationUser user in Model)
				{
					<tr id="@("ID" + user.Id)">
						<td>@user.FirstName</td>
						<td>@user.LastName</td>
					</tr>
				}
		</tbody>
	</table>
	} else
	{
	<table class="table table-striped">
		<tbody>
				<tr><td>Vide</td></tr>
		</tbody>
	</table>
	}
</div>
